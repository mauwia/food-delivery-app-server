    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/platform-socket.io": "^7.6.13",
    "@nestjs/websockets": "^7.6.5",
    "@turf/distance": "^6.3.0",
    "@turf/helpers": "^6.3.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "exceljs": "^4.2.1",
    "firebase-admin": "^9.4.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.9",
    "mongoose-autopopulate": "^0.12.3",
    "mongoose-paginate-v2": "^1.3.18",
    "multer": "^1.4.2",
    "nest-router": "^1.0.9",
    "nestjs-twilio": "^1.0.2",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "tmp": "^0.0.33",
    "ts-node": "^9.1.1",
    "twilio": "^3.54.2",
    "web3": "^1.3.6"
